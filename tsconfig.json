{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "world-countries": [
        "./types/world-countries/index.d.ts"
      ],
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "next-env.d.js",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/page.jsx",
    "app/loading.jsx",
    "app/layout.jsx",
    "app/error.jsx",
    "app/reservations/page.jsx",
    "app/reservations/ReservationsClient.jsx",
    "app/properties/page.jsx",
    "app/properties/PropertiesClient.jsx",
    "app/listings/[listingId]/page.jsx",
    "app/favorites/page.jsx",
    "app/favorites/FavoritesClient.jsx",
    "components/inputs/ImageUpload.tsx",
    "components/inputs/Input.tsx",
    "components/inputs/Counter.tsx",
    "components/inputs/Calendar.tsx",
    "components/listing/ListingCard.tsx",
    "components/listing/ListingHead.tsx",
    "components/listing/ListingInfo.tsx",
    "components/listing/ListingReservation.tsx",
    "components/listing/ListingComments.tsx",
    "components/models/LoginModal.tsx",
    "components/models/Modal.jsx",
    "components/models/RegisterModal.jsx",
    "components/models/RentModal.jsx",
    "components/models/SearchModal.tsx",
    "components/navbar/UserMenu.tsx",
    "components/navbar/Search.tsx",
    "components/navbar/MenuItem.tsx",
    "components/navbar/Navbar.tsx",
    "components/navbar/Logo.tsx",
    "components/Avatar.tsx",
    "components/Button.tsx",
    "components/CategoryBox.tsx",
    "components/ClientOnly.tsx",
    "components/Container.tsx",
    "components/EmptyState.tsx",
    "components/Footer.tsx",
    "components/FooterColumn.tsx",
    "components/Heading.tsx",
    "components/HeartButton.tsx",
    "components/ListingClient.tsx",
    "components/Loader.tsx",
    "components/Map.tsx",
    "components/Offers.tsx",
    "components/Sleep.tsx",
    "components/ToastContainerBar.tsx",
    "hook/useLoginModal.ts",
    "hook/useRegisterModal.ts",
    "hook/useRentModal.ts",
    "hook/useSearchModal.ts"
  ],
  "exclude": [
    "node_modules"
  ],
}