{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "world-countries": [
        "./types/world-countries/index.d.ts"
      ],
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "next-env.d.js",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/page.jsx",
    "app/loading.jsx",
    "app/layout.jsx",
    "app/error.jsx",
    "app/reservations/page.jsx",
    "app/reservations/ReservationsClient.jsx",
    "app/properties/page.jsx",
    "app/properties/PropertiesClient.jsx",
    "app/listings/[listingId]/page.jsx",
    "app/favorites/page.jsx",
    "app/favorites/FavoritesClient.jsx",
    "components/inputs/ImageUpload.jsx",
    "components/inputs/Input.jsx",
    "components/inputs/Counter.jsx",
    "components/inputs/Calendar.jsx",
    "components/listing/ListingCard.jsx",
    "components/listing/ListingHead.jsx",
    "components/listing/ListingInfo.jsx",
    "components/listing/ListingReservation.jsx",
    "components/listing/ListingComments.jsx",
    "components/models/LoginModal.jsx",
    "components/models/Modal.jsx",
    "components/models/RegisterModal.jsx",
    "components/models/RentModal.jsx",
    "components/models/SearchModal.jsx",
    "components/navbar/UserMenu.jsx",
    "components/navbar/Search.jsx",
    "components/navbar/MenuItem.jsx",
    "components/navbar/Navbar.jsx",
    "components/navbar/Logo.jsx",
    "components/Avatar.tsx",
    "components/Button.tsx",
    "components/CategoryBox.tsx",
    "components/ClientOnly.tsx",
    "components/Container.tsx",
    "components/EmptyState.tsx",
    "components/Footer.tsx",
    "components/FooterColumn.tsx",
    "components/Heading.tsx",
    "components/HeartButton.tsx",
    "components/ListingClient.jsx",
    "components/Loader.jsx",
    "components/Map.jsx",
    "components/Offers.jsx",
    "components/Sleep.tsx",
    "components/ToastContainerBar.tsx",
    "hook/useLoginModal.js",
    "hook/useRegisterModal.js",
    "hook/useRentModal.js",
    "hook/useSearchModal.js"
  ],
  "exclude": [
    "node_modules"
  ],
}